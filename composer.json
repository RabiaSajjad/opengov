{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "ckeditor/codemirror",
                "version" : "v1.17.10",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/w8tcha/CKEditor-CodeMirror-Plugin/releases/download/v1.17.10/CKEditor-CodeMirror-Plugin.zip",
                    "type" : "zip"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/wet-boew",
                "version" : "v4.0.30",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/wet-boew-cdn/archive/v4.0.30.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/wet-boew-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.30"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-gcweb",
                "version" : "v5.0.1",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v5.0.1-gcweb.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v5.0.1-gcweb"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.25",
        "drupal/bootstrap": "^3.17",
        "drupal/bootstrap_layouts": "5.1.0",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/feeds": "^3.0@alpha",
        "drupal/fontawesome": "^2.9",
        "drupal/menu_breadcrumb": "^1.8",
        "drupal/metatag": "^1.7",
        "drupal/pathauto": "^1.3",
        "drupal/redirect": "^1.3",
        "drupal/webform": "^5.2",
        "drush/drush": "^9.0.0",
        "fortawesome/font-awesome": "^5.7",
        "oomphinc/composer-installers-extender": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wet-boew/theme-gcweb": "^5.0",
        "wet-boew/wet-boew": "v4.0.30",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": false,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/bootstrap": {
                "Error: Unsupported operand types during providers JSON discovery": "https://www.drupal.org/files/issues/2019-02-04/3027569-23.patch"
            },
            "drupal/core": {
                "Submitting a comment in the non default language redirects you to the default language": "https://www.drupal.org/files/issues/2019-05-08/2751269-redirect_to_current_node_language_after_post_comment-24.patch",
                "Comments created in translation are displayed only for admin role": "https://www.drupal.org/files/issues/2018-04-13/drupal-comment_translation_displayed_only_admin-2958935-9-D8.patch"
            },
            "drupal/feeds": {
                "Language support in Feed import - allows make a mapping of language" : "https://www.drupal.org/files/issues/2018-11-01/feeds-language-target-2829283-31.patch",
                "Language support in Feed import - content translation": "https://www.drupal.org/files/issues/2019-01-26/feeds-content-translation-2829283-42.patch"
            }
        },
       "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "html/core": ["type:drupal-core"],
            "html/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "html/modules/contrib/{$name}": ["type:drupal-module"],
            "html/profiles/contrib/{$name}": ["type:drupal-profile"],
            "html/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
